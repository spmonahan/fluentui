{
  "name": "@fluentui/stress-test",
  "version": "8.0.0",
  "description": "Stress Tests for Fluent UI",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/fluentui"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc && vite build",
    "clean": "just-scripts clean",
    "code-style": "just-scripts code-style",
    "lint": "just-scripts lint",
    "test": "jest --passWithNoTests",
    "start": "vite",
    "type-check": "tsc -b tsconfig.json",
    "serve": "http-server ./dist -o",
    "bench:mount": "tach --config ./benchmarks/simple-stress.mount.json --json-file ./benchmarks/results/results.simple-stress.mount.json",
    "bench:inject-styles": "tach --config ./benchmarks/simple-stress.inject-styles.json --json-file ./benchmarks/results/results.simple-stress.inject-styles.json"
  },
  "dependencies": {
    "@fluentui/react": "^8.72.3",
    "@fluentui/react-components": "^9.1.0",
    "@fluentui/react-icons": "^2.0.177",
    "@fluentui/web-components": "^2.5.3",
    "@microsoft/fast-element": "^1.10.4",
    "clsx": "^1.2.1",
    "mobx": "^6.6.1",
    "mobx-react": "^7.5.1",
    "random-seedable": "^1.0.8",
    "react": ">=16.8.0 <18.0.0",
    "react-dom": ">=16.8.0 <18.0.0",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "^1.8.7",
    "tachometer": "^0.7.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@fluentui/eslint-plugin": "*",
    "@fluentui/scripts": "^1.0.0",
    "@types/react": ">=16.8.0 <18.0.0",
    "@types/react-dom": ">=16.8.0 <18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "http-server": "^14.1.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
  }
}
